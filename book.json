{
  "title": "VertX核心手册Java版",
  "description": "VertX核心手册Java版",
  "author": "WS",
  "language": "zh-hans",
  "plugins": [
    "github@^2.0.0",
    "edit-link@^2.0.2",
    "anchors@^0.7.1",
    "include-codeblock@^3.0.2",
    "splitter@^0.0.8",
    "tbfed-pagefooter@^0.0.1",
    "expandable-chapters-small@^0.1.7",
    "anchor-navigation-ex@0.1.8",
    "book-summary-scroll-position-saver",
    "ace", "emphasize",
    "-lunr", "-search", "search-plus"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/wjw465150/Mastering-Bitcoin-2nd-Edition-Chinese-Simplified-"
    },
    "edit-link": {
      "base": "https://github.com/wjw465150/Mastering-Bitcoin-2nd-Edition-Chinese-Simplified-/edit/master",
      "label": "编辑此页面"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © WS 2020",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
      "showLevel": false
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "edit": true
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}

